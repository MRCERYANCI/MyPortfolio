@model List<MyPortfolio.DAL.Entities.SocialMedia>

<ul class="footer-social">
    @foreach(var SocialMedias in Model)
    {
        <li>
            <a href="@SocialMedias.Url" target="_blank">
                <i class="@SocialMedias.Icon" aria-hidden="true"></i>
                <span>@SocialMedias.Title</span>
            </a>
        </li>
    }
</ul>